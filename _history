s_x[i], others_y[i])\n\n# Send all info in one string (concatenation instead of format)\ndef send_position():\n    msg = str(my_id) + \",\" + str(my_x) + \",\" + str(my_y) + \",\" + str(code)\n    radio.send_string(msg)\n\n# Button handlers\ndef on_button_a():\n    global my_x\n    my_x = (my_x + 1) % 5\n    send_position()\n    draw_dots()\ninput.on_button_pressed(Button.A, on_button_a)\n\ndef on_button_b():\n    global my_y\n    my_y = (my_y + 1) % 5\n    send_position()\n    draw_dots()\ninput.on_button_pressed(Button.B, on_button_b)\n\ndef on_button_ab():\n    global code, my_x, my_y\n    code += 1\n    basic.show_number(code)\n    send_position()\n    draw_dots()\ninput.on_button_pressed(Button.AB, on_button_ab)\n\n# Receive and parse message\ndef on_received_string(msg):\n    global others_ids, others_x, others_y, others_code\n    try:\n        parts = msg.split(\",\")\n        sender_id = int(parts[0])\n        x = int(parts[1])\n        y = int(parts[2])\n        sender_code = int(parts[3])\n\n        if sender_id == my_id:\n            return  # ignore self\n\n        # Add new player if not tracked\n        if sender_id not in others_ids:\n            others_ids.append(sender_id)\n            others_x.append(x)\n            others_y.append(y)\n            others_code.append(sender_code)\n        else:\n            idx = others_ids.index(sender_id)\n            others_x[idx] = x\n            others_y[idx] = y\n            others_code[idx] = sender_code\n\n        # Remove players in different codes\n        for i in range(len(others_ids)-1, -1, -1):\n            if others_code[i] != code:\n                others_ids.pop(i)\n                others_x.pop(i)\n                others_y.pop(i)\n                others_code.pop(i)\n\n        draw_dots()\n    except:\n        pass  # ignore malformed messages\n\nradio.on_received_string(on_received_string)","pxt.json":"{\n    \"name\": \"Multiplayer\",\n    \"description\": \"\",\n    \"dependencies\": {\n        \"